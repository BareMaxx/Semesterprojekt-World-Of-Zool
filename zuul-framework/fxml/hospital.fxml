<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HospitalController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/dkusaHospitalBG.png" />
         </image>
      </ImageView>
      <ImageView fx:id="doorImage" fitHeight="394.0" fitWidth="236.0" layoutX="-1.0" layoutY="71.0" onMouseClicked="#goOutside" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/dkusaHospitalDoor.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="370.0" fitWidth="694.0" layoutX="278.0" layoutY="348.0" onMouseClicked="#heal" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/dkusaHospitalBed.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
