<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SchoolController">
   <children>
      <ImageView fx:id="backgroundImage" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/ugSchoolBG.png" />
         </image>
      </ImageView>
      <ImageView fx:id="doorImage" fitHeight="509.0" fitWidth="116.0" layoutX="1048.0" layoutY="39.0" onMouseClicked="#goOutside" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/ugSchoolDoor.png" />
         </image>
      </ImageView>
      <ImageView fx:id="bookImage" fitHeight="34.0" fitWidth="116.0" layoutX="768.0" layoutY="406.0" onMouseClicked="#readBook" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../png/schoolBook.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
